{
  "name": "i18nify",
  "version": "1.0.0-alpha",
  "description": "i18nify is a tool that provides internalization features to existing systems and apps by making static texts into translatable texts.",
  "main": "index.js",
  "repository": "https://github.com/batrdn/i18nify",
  "author": "batrdn <baterdene.tsogoo220@gmail.com>",
  "license": "Apache",
  "scripts": {
    "build": "tsc",
    "server": "ts-node src/server/index.ts",
    "format": "prettier -u --check .",
    "format:fix": "prettier -u --write .",
    "lint": "eslint src --ext .js,.ts,",
    "lint-staged": "lint-staged",
    "start": "webpack serve --open",
    "test": "jest",
    "test:coverage": "yarn test --coverage --colors",
    "prepare": "husky install && yarn build",
    "tsc:check": "tsc --noEmit",
    "prepublish": "yarn build",
    "release": "semantic-release"
